<?xml version="1.0" encoding="utf-8"?>
<datasheet javaPackage="jp.co.daifuku.wms.stock.rft.datasheet" xmlns="urn:busitune4rft:datasheet:1.0">
  <comment lang="ja" value="予定外出庫入力データシート" />
  <comment lang="en" value="Unplanned Picking Input Data Sheet" />
  <comment lang="zh" value="预定外出库输入数据表" />
  <fields>
    <field name="AREA_NO" type="String" separateSize="0">
      <comment lang="ja" value="エリアNo." />
      <comment lang="en" value="Area #" />
      <comment lang="zh" value="区域No." />
    </field>
    <field name="CONSIGNOR_CODE" type="String" separateSize="0">
      <comment lang="ja" value="荷主コード" />
      <comment lang="en" value="Consignor Code" />
      <comment lang="zh" value="货主代码" />
    </field>
    <field name="ITEM_CODE" type="String" separateSize="0">
      <comment lang="ja" value="商品コード" />
      <comment lang="en" value="Item Code" />
      <comment lang="zh" value="商品代码" />
    </field>
    <field name="LOCATION_NO" type="String" separateSize="0">
      <comment lang="ja" value="棚No." />
      <comment lang="en" value="Location #" />
      <comment lang="zh" value="货架No." />
    </field>
    <field name="LOT_NO" type="String" separateSize="0" />
    <field name="LOT_NO_SELECT" type="Boolean" separateSize="0" />
    <field name="RESULT_CASE_QTY" type="Number" format="0" separateSize="0">
      <comment lang="ja" value="実績ケース数" />
      <comment lang="en" value="Result Case Qty" />
      <comment lang="zh" value="实绩盒装数" />
    </field>
    <field name="RESULT_LOT_NO" type="String" separateSize="0">
      <comment lang="ja" value="ロットNo." />
      <comment lang="en" value="Lot #" />
      <comment lang="zh" value="批号" />
    </field>
    <field name="RESULT_PIECE_QTY" type="Number" format="0" separateSize="0">
      <comment lang="ja" value="実績ピース数" />
      <comment lang="en" value="Result Piece Qty" />
      <comment lang="zh" value="实绩散装数" />
    </field>
    <field name="SCAN_CODE" type="String" separateSize="0">
      <comment lang="ja" value="スキャンコード" />
      <comment lang="en" value="Scan Code" />
      <comment lang="zh" value="扫描代码" />
    </field>
  </fields>
</datasheet>