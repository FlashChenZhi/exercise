<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--数量入力画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <input name="inputCaseQty" type="numeric" count-max="4" />
  <input name="inputPieceQty" type="numeric" count-max="4" />
  <input name="inputLotNo" type="non" count-max="16" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0" focus="%CommonDataSheet.FOCUS_FIELD%">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>right</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>数量入力             </caption>
    </label>
    <button name="Back" font-id="font12r" visible="true" enabled="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:戻る      </caption>
      <key>KL</key>
    </button>
    <label name="lblItemCode" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>14</y>
      <w>20</w>
      <caption>%xxxxOutDataSheet.ITEM_CODE%</caption>
    </label>
    <label name="lblItemName1" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>27</y>
      <w>20</w>
      <caption>%xxxxOutDataSheet.ITEM_NAME0%</caption>
    </label>
    <label name="lblItemName2" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>40</y>
      <w>14</w>
      <caption>%xxxxOutDataSheet.ITEM_NAME1%</caption>
    </label>
    <label name="lblEnteringQty" font-id="font12" visible="true">
      <align>right</align>
      <x>90</x>
      <y>40</y>
      <w>3</w>
      <caption>%xxxxOutDataSheet.ENTERING_QTY%</caption>
    </label>
    <label name="lblEntering" font-id="font12" visible="true">
      <align>left</align>
      <x>108</x>
      <y>40</y>
      <w>
      </w>
      <caption>入</caption>
    </label>
    <label name="lblCase" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>53</y>
      <w>
      </w>
      <caption>ｹｰｽ数: [     ](    )</caption>
    </label>
    <label name="lblPiece" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>66</y>
      <w>
      </w>
      <caption>ﾋﾟｰｽ数:[     ](    )</caption>
    </label>
    <label name="lblEdit" font-id="" visible="true">
      <align>left</align>
      <x>0</x>
      <y>79</y>
      <w>
      </w>
      <caption>[                 ]</caption>
    </label>
    <edit name="LotNo" enabled="true" font-id="font12" input-id="inputLotNo" visible="true" password="false" scan="true" keyence-ok-alarm="non" keyence-ok-vibration="false" keyence-ok-led="non" keyence-ng-alarm="non" keyence-ng-vibration="false" keyence-ng-led="non">
      <x>6</x>
      <y>79</y>
      <w>16</w>
      <text>%xxxxOutDataSheet.RESULT_LOT_NO%</text>
    </edit>
    <edit name="CaseQty" enabled="%CommonDataSheet.CASE_INPUT_ENABLED%" font-id="font12" input-id="inputCaseQty" visible="true" password="false" scan="false" keyence-ok-alarm="non" keyence-ok-vibration="false" keyence-ok-led="non" keyence-ng-alarm="non" keyence-ng-vibration="false" keyence-ng-led="non" keyence-align="right">
      <x>50</x>
      <y>53</y>
      <w>5</w>
      <text>%xxxxOutDataSheet.CASE_QTY%</text>
    </edit>
    <edit name="PieceQty" enabled="true" font-id="font12" input-id="inputPieceQty" visible="true" password="false" scan="false" keyence-ok-alarm="non" keyence-ok-vibration="false" keyence-ok-led="non" keyence-ng-alarm="non" keyence-ng-vibration="false" keyence-ng-led="non" keyence-align="right">
      <x>50</x>
      <y>66</y>
      <w>5</w>
      <text>%xxxxOutDataSheet.PIECE_QTY%</text>
    </edit>
    <label name="lblMsg" font-id="" visible="true">
      <align>left</align>
      <x>0</x>
      <y>118</y>
      <w>
      </w>
      <caption>数量を入力して下さい</caption>
    </label>
    <label name="lblLot" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>92</y>
      <w>
      </w>
      <caption>(                )</caption>
    </label>
    <label name="lblPlanCase" font-id="font12" visible="true">
      <align>right</align>
      <x>90</x>
      <y>53</y>
      <w>4</w>
      <caption>%xxxxOutDataSheet.PLAN_CASE_QTY%</caption>
    </label>
    <label name="lblPlanPiece" font-id="font12" visible="true">
      <align>right</align>
      <x>90</x>
      <y>66</y>
      <w>4</w>
      <caption>%xxxxOutDataSheet.PLAN_PIECE_QTY%</caption>
    </label>
    <label name="lblPlanLot" font-id="font12" visible="true">
      <align>left</align>
      <x>6</x>
      <y>92</y>
      <w>16</w>
      <caption>%xxxxOutDataSheet.PLAN_LOT_NO%</caption>
    </label>
    <button name="Return" font-id="" visible="false" enabled="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <key>KR</key>
    </button>
    <label name="lblEnt" font-id="font12r" visible="true">
      <align>left</align>
      <x>71</x>
      <y>105</y>
      <w>
      </w>
      <caption>ENT:確定</caption>
    </label>
  </form>
</HaiSurf.Page>