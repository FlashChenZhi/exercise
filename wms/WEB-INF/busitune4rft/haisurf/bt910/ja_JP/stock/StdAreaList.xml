<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--DC入庫エリア一覧画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>right</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>エリア一覧     %AreaListDataSheet.$PAGE%/%AreaListDataSheet.$NUM_PAGES%</caption>
    </label>
    <label name="lblAreaNo1" font-id="%AreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>14</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[0]%</caption>
    </label>
    <label name="lblAreaNo2" font-id="%AreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>27</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[1]%</caption>
    </label>
    <label name="lblAreaNo3" font-id="%AreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>40</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[2]%</caption>
    </label>
    <label name="lblAreaNo4" font-id="%AreaListDataSheet.@FONT[3]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>53</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[3]%</caption>
    </label>
    <label name="lblAreaNo5" font-id="%AreaListDataSheet.@FONT[4]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>66</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[4]%</caption>
    </label>
    <label name="lblAreaNo6" font-id="%AreaListDataSheet.@FONT[5]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>79</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[5]%</caption>
    </label>
    <label name="lblMsg" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>118</y>
      <w>
      </w>
      <caption>エリアを選択</caption>
    </label>
    <button name="Prev" font-id="" visible="false" enabled="%AreaListDataSheet.$PREV_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>AreaL_Prev</event>
      <key>LT</key>
    </button>
    <button name="Next" font-id="" visible="false" enabled="%AreaListDataSheet.$NEXT_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>AreaL_Next</event>
      <key>RT</key>
    </button>
    <button name="Up" font-id="" visible="false" enabled="%AreaListDataSheet.$UP_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>AreaL_Up</event>
      <key>UP</key>
    </button>
    <button name="Down" font-id="" visible="false" enabled="%AreaListDataSheet.$DOWN_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>AreaL_Down</event>
      <key>DW</key>
    </button>
    <button name="Ent" font-id="font12r" visible="true" enabled="true" inputchecked="false">
      <x>66</x>
      <y>105</y>
      <w>
      </w>
      <caption> ENT:選択</caption>
      <event>AreaL_Ent</event>
      <key>ET</key>
    </button>
    <button name="Back" font-id="font12r" visible="true" enabled="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:戻る     </caption>
      <event>AreaL_Back</event>
      <key>KL</key>
    </button>
    <label name="lblAreaName1" font-id="%AreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>14</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[0]%</caption>
    </label>
    <label name="lblAreaName2" font-id="%AreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>27</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[1]%</caption>
    </label>
    <label name="lblAreaName3" font-id="%AreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>40</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[2]%</caption>
    </label>
    <label name="lblAreaName4" font-id="%AreaListDataSheet.@FONT[3]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>53</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[3]%</caption>
    </label>
    <label name="lblAreaName5" font-id="%AreaListDataSheet.@FONT[4]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>66</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[4]%</caption>
    </label>
    <label name="lblAreaName6" font-id="%AreaListDataSheet.@FONT[5]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>79</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[5]%</caption>
    </label>
    <label name="lblAreaNo7" font-id="%AreaListDataSheet.@FONT[6]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>92</y>
      <w>20</w>
      <caption>%AreaListDataSheet.@AREA_NO[6]%</caption>
    </label>
    <label name="lblAreaName7" font-id="%AreaListDataSheet.@FONT[6]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>92</y>
      <w>
      </w>
      <caption>%AreaListDataSheet.@AREA_NAME[6]%</caption>
    </label>
  </form>
</HaiSurf.Page>