      <y>0</y>
      <w>
      </w>
      <event>NrtStockL_Down</event>
      <key>DW</key>
    </button>
    <button name="Prev" font-id="" visible="true" enabled="%NoPlanRetrievalStockListDataSheet.$PREV_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>NrtStockL_Prev</event>
      <key>LT</key>
    </button>
    <button name="Next" font-id="" visible="true" enabled="%NoPlanRetrievalStockListDataSheet.$NEXT_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>NrtStockL_Next</event>
      <key>RT</key>
    </button>
  </form>
