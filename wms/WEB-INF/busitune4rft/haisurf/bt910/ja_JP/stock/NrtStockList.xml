      <y>0</y>
      <w>
      </w>
      <event>NrtStockL_Up</event>
      <key>UP</key>
    </button>
    <button name="Down" font-id="" visible="true" enabled="%NoPlanRetrievalStockListDataSheet.$DOWN_ENABLED%" inputchecked="true">
