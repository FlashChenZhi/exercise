<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--荷主一覧画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>right</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>荷主一覧       %ConsignorListDataSheet.$PAGE%/%ConsignorListDataSheet.$NUM_PAGES%</caption>
    </label>
    <label name="lblConsignorCode1" font-id="%ConsignorListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>14</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_CODE[0]%</caption>
    </label>
    <label name="lblConsignorName1" font-id="%ConsignorListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>26</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_NAME[0]%</caption>
    </label>
    <label name="lblConsignorCode2" font-id="%ConsignorListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>40</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_CODE[1]%</caption>
    </label>
    <label name="lblConsignorName2" font-id="%ConsignorListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>52</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_NAME[1]%</caption>
    </label>
    <label name="lblConsignorCode3" font-id="%ConsignorListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>66</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_CODE[2]%</caption>
    </label>
    <label name="lblConsignorName3" font-id="%ConsignorListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>78</y>
      <w>20</w>
      <caption>%ConsignorListDataSheet.@CONSIGNOR_NAME[2]%</caption>
    </label>
    <label name="lblMsg" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>118</y>
      <w>
      </w>
      <caption>荷主を選択</caption>
    </label>
    <button name="Prev" font-id="" visible="false" enabled="%ConsignorListDataSheet.$PREV_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>ConsignorL_Prev</event>
      <key>LT</key>
    </button>
    <button name="Next" font-id="" visible="false" enabled="%ConsignorListDataSheet.$NEXT_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>ConsignorL_Next</event>
      <key>RT</key>
    </button>
    <button name="Up" font-id="" visible="false" enabled="%ConsignorListDataSheet.$UP_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>ConsignorL_Up</event>
      <key>UP</key>
    </button>
    <button name="Down" font-id="" visible="false" enabled="%ConsignorListDataSheet.$DOWN_ENABLED%" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>ConsignorL_Down</event>
      <key>DW</key>
    </button>
    <button name="Ent" font-id="font12r" visible="true" enabled="true" inputchecked="false">
      <x>66</x>
      <y>105</y>
      <w>
      </w>
      <caption> ENT:選択</caption>
      <event>ConsignorL_Ent</event>
      <key>ET</key>
    </button>
    <button name="Back" font-id="font12r" visible="true" enabled="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:戻る     </caption>
      <event>ConsignorL_Back</event>
      <key>KL</key>
    </button>
  </form>
</HaiSurf.Page>