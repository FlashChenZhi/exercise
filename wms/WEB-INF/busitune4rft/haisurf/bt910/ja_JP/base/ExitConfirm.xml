<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--終了確認ダイアログ画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>確認                 </caption>
    </label>
    <label name="lblMsg" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>27</y>
      <w>
      </w>
      <caption>作業を終了しますか？</caption>
    </label>
    <button name="Yes" font-id="font12r" visible="true" enabled="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:はい      </caption>
      <event>ExitConfirm_Yes</event>
      <key>KL</key>
    </button>
    <button name="No" font-id="font12r" visible="true" enabled="true" inputchecked="true">
      <x>72</x>
      <y>105</y>
      <w>
      </w>
      <caption>R:いいえ</caption>
      <event>ExitConfirm_No</event>
      <key>KR</key>
    </button>
  </form>
</HaiSurf.Page>