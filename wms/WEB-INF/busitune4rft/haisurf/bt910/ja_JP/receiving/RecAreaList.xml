<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--DC入荷場エリア一覧画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>right</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>エリア一覧     %ReceivingAreaListDataSheet.$PAGE%/%ReceivingAreaListDataSheet.$NUM_PAGES%</caption>
    </label>
    <label name="lblTempAreaNo1" font-id="%ReceivingAreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>14</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NO[0]%</caption>
    </label>
    <label name="lblAreaNo1" font-id="%ReceivingAreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>26</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NO[0]%</caption>
    </label>
    <label name="lblTempAreaNo2" font-id="%ReceivingAreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>40</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NO[1]%</caption>
    </label>
    <label name="lblAreaNo2" font-id="%ReceivingAreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>52</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NO[1]%</caption>
    </label>
    <label name="lblTempArea3" font-id="%ReceivingAreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>66</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NO[2]%</caption>
    </label>
    <label name="lblAreaNo3" font-id="%ReceivingAreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>78</y>
      <w>20</w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NO[2]%</caption>
    </label>
    <label name="lblMsg" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>118</y>
      <w>
      </w>
      <caption>エリアを選択</caption>
    </label>
    <button name="Prev" font-id="" visible="false" enabled="%ReceivingAreaListDataSheet.$PREV_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>RecAreaL_Prev</event>
      <key>LT</key>
    </button>
    <button name="Next" font-id="" visible="false" enabled="%ReceivingAreaListDataSheet.$NEXT_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>RecAreaL_Next</event>
      <key>RT</key>
    </button>
    <button name="Up" font-id="" visible="false" enabled="%ReceivingAreaListDataSheet.$UP_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>RecAreaL_Up</event>
      <key>UP</key>
    </button>
    <button name="Down" font-id="" visible="false" enabled="%ReceivingAreaListDataSheet.$DOWN_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>RecAreaL_Down</event>
      <key>DW</key>
    </button>
    <button name="Ent" font-id="font12r" visible="true" enabled="true" modeonline="true" inputchecked="false">
      <x>66</x>
      <y>105</y>
      <w>
      </w>
      <caption> ENT:選択</caption>
      <event>RecAreaL_Ent</event>
      <key>ET</key>
    </button>
    <button name="Back" font-id="font12r" visible="true" enabled="true" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:戻る     </caption>
      <event>RecAreaL_Back</event>
      <key>KL</key>
    </button>
    <label name="lblTempAreaName1" font-id="%ReceivingAreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>14</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NAME[0]%</caption>
    </label>
    <label name="lblAreaName1" font-id="%ReceivingAreaListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>26</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NAME[0]%</caption>
    </label>
    <label name="lblTempAreaName2" font-id="%ReceivingAreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>40</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NAME[1]%</caption>
    </label>
    <label name="lblAreaName2" font-id="%ReceivingAreaListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>52</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NAME[1]%</caption>
    </label>
    <label name="lblTempAreaName3" font-id="%ReceivingAreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>66</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@TEMP_AREA_NAME[2]%</caption>
    </label>
    <label name="lblAreaName3" font-id="%ReceivingAreaListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>30</x>
      <y>78</y>
      <w>
      </w>
      <caption>%ReceivingAreaListDataSheet.@AREA_NAME[2]%</caption>
    </label>
  </form>
</HaiSurf.Page>