<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--予定日一覧画面-->
  <font name="font12" keyence_mag="1" size="12" />
  <font name="font12r" keyence_mag="1" size="12" keyence_reverse="true" />
  <form keyence-alarm="non" keyence-led="non" statusline="0" kanainput="0">
    <label name="line" font-id="font12r" visible="true">
      <align>left</align>
      <x>0</x>
      <y>1</y>
      <w>20</w>
    </label>
    <label name="lblTitle" font-id="font12" visible="true">
      <align>right</align>
      <x>0</x>
      <y>0</y>
      <w>20</w>
      <caption>%CommonDataSheet.JOB_TYPE_NAME%日一覧     %PlanDateListDataSheet.$PAGE%/%PlanDateListDataSheet.$NUM_PAGES%</caption>
    </label>
    <label name="PlanDate1" font-id="%PlanDateListDataSheet.@FONT[0]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>14</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[0]%</caption>
    </label>
    <label name="PlanDate2" font-id="%PlanDateListDataSheet.@FONT[1]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>27</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[1]%</caption>
    </label>
    <label name="PlanDate3" font-id="%PlanDateListDataSheet.@FONT[2]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>40</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[2]%</caption>
    </label>
    <label name="PlanDate4" font-id="%PlanDateListDataSheet.@FONT[3]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>53</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[3]%</caption>
    </label>
    <label name="PlanDate5" font-id="%PlanDateListDataSheet.@FONT[4]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>66</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[4]%</caption>
    </label>
    <label name="PlanDate6" font-id="%PlanDateListDataSheet.@FONT[5]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>79</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[5]%</caption>
    </label>
    <label name="PlanDate7" font-id="%PlanDateListDataSheet.@FONT[6]%" visible="true">
      <align>left</align>
      <x>0</x>
      <y>92</y>
      <w>20</w>
      <caption>%PlanDateListDataSheet.@PLAN_DATE[6]%</caption>
    </label>
    <label name="lblMsg" font-id="font12" visible="true">
      <align>left</align>
      <x>0</x>
      <y>118</y>
      <w>
      </w>
      <caption>日付を選択</caption>
    </label>
    <button name="Prev" font-id="" visible="false" enabled="%PlanDateListDataSheet.$PREV_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>PlanDateL_Prev</event>
      <key>LT</key>
    </button>
    <button name="Next" font-id="" visible="false" enabled="%PlanDateListDataSheet.$NEXT_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>PlanDateL_Next</event>
      <key>RT</key>
    </button>
    <button name="Up" font-id="" visible="false" enabled="%PlanDateListDataSheet.$UP_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>PlanDateL_Up</event>
      <key>UP</key>
    </button>
    <button name="Down" font-id="" visible="false" enabled="%PlanDateListDataSheet.$DOWN_ENABLED%" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>0</y>
      <w>
      </w>
      <event>PlanDateL_Down</event>
      <key>DW</key>
    </button>
    <button name="Ent" font-id="font12r" visible="true" enabled="true" modeonline="true" inputchecked="false">
      <x>66</x>
      <y>105</y>
      <w>
      </w>
      <caption> ENT:選択</caption>
      <event>PlanDateL_Ent</event>
      <key>ET</key>
    </button>
    <button name="Back" font-id="font12r" visible="true" enabled="true" modeonline="true" inputchecked="true">
      <x>0</x>
      <y>105</y>
      <w>
      </w>
      <caption>L:戻る     </caption>
      <event>PlanDateL_Back</event>
      <key>KL</key>
    </button>
  </form>
</HaiSurf.Page>