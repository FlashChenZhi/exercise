<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--ErrorDialog(End/Continue)-->
  <font name="fontTitle" fontname="Tahoma" bold="true" italic="false" size="18" bht_size="1" />
  <font name="fontStandard" fontname="Tahoma" bold="true" italic="false" size="18" bht_size="1" />
  <font name="fontSmall" fontname="Tahoma" bold="false" italic="false" size="18" bht_size="1" />
  <font name="fontBtn" fontname="Tahoma" bold="true" italic="false" size="14" bht_size="1" />
  <form color="E6E6FA" wave="Platform\\WavAlias\\EMPTY.WAV" vibration="false" bht-alarm="non" bht-led="non" ce-alarm="non" ce-led="non" controlbox="false" titlebar="false">
    <label name="lblTitle" font-id="fontTitle" visible="true" forecolor="FF0000">
      <align>center</align>
      <x>0</x>
      <y>15</y>
      <w>320</w>
      <h>40</h>
      <caption>&lt;&lt;Error&gt;&gt;</caption>
    </label>
    <label name="ErrMessage" font-id="fontStandard" visible="true" forecolor="FF0000" backcolor="C8C8FA">
      <align>center</align>
      <x>0</x>
      <y>90</y>
      <w>320</w>
      <h>60</h>
      <caption>%BusinessControlDataSheet.MESSAGE%</caption>
    </label>
    <label name="SubErrorMessage" font-id="fontSmall" visible="true" forecolor="FF0000" backcolor="C8C8FA">
      <align>center</align>
      <x>0</x>
      <y>160</y>
      <w>320</w>
      <h>60</h>
      <caption>%BusinessControlDataSheet.SUBMESSAGE%</caption>
    </label>
    <button name="Exit" font-id="fontBtn" visible="true" backcolor="FFA500" enabled="true" inputchecked="false">
      <x>0</x>
      <y>275</y>
      <w>162</w>
      <h>45</h>
      <caption>End</caption>
      <event>ErrorB_Exit</event>
    </button>
    <button name="Ent" font-id="fontBtn" visible="true" backcolor="FFA500" enabled="true" inputchecked="false">
      <x>162</x>
      <y>275</y>
      <w>162</w>
      <h>45</h>
      <caption>Continue</caption>
      <event>ErrorB_Ent</event>
    </button>
    <label name="panel" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>0</x>
      <y>70</y>
      <w>324</w>
      <h>168</h>
    </label>
  </form>
</HaiSurf.Page>
