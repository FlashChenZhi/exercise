      <text>%CommonDataSheet.USER_ID%</text>
      <event>WorkerI_UserEnt</event>
    </edit>
    <label name="lblPassword" font-id="fontStandard" visible="%TermInfoDataSheet.PASSWORD_INPUT_MODE%">
      <align>left</align>
      <x>4</x>
      <y>84</y>
      <w>124</w>
      <h>20</h>
      <caption>Password:</caption>
    </label>
