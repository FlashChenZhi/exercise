      <text>%CommonDataSheet.PASSWORD%</text>
      <event>WorkerI_PasswordEnt</event>
    </edit>
    <label name="lblUser" font-id="fontSmall" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>6</x>
      <y>120</y>
      <w>100</w>
      <h>14</h>
      <caption>User:</caption>
    </label>
    <label name="lblUserId" font-id="fontStandard" visible="true" backcolor="C8C8FA">
