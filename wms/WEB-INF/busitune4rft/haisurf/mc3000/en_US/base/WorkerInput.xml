  <font name="fontBtn" fontname="Tahoma" bold="true" italic="false" size="14" bht_size="1" />
  <scan type="focus" />
  <form color="E6E6FA" focus="%CommonDataSheet.FocusField%" wave="%CommonDataSheet.BeepWav%" vibration="true" bht-alarm="non" bht-led="non" ce-alarm="non" ce-led="non" controlbox="false" titlebar="false">
    <label name="lblTitleBar" font-id="fontTitleBar" visible="true" forecolor="FFFFFF" backcolor="0000C0">
