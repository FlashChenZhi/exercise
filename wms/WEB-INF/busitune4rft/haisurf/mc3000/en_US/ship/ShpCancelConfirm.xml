<?xml version="1.0" encoding="utf-8"?>
<HaiSurf.Page>
  <!--ShippingCancelConfirmation-->
  <font name="fontTitle" fontname="Tahoma" bold="true" italic="false" size="18" bht_size="1" />
  <font name="fontStandard" fontname="Tahoma" bold="true" italic="false" size="13" bht_size="1" />
  <font name="fontBtn" fontname="Tahoma" bold="true" italic="false" size="14" bht_size="1" />
  <font name="fontSmall" fontname="Courier New" bold="false" italic="false" size="12" bht_size="1" />
  <form color="E6E6FA" vibration="false" bht-alarm="non" bht-led="non" ce-alarm="non" ce-led="non" controlbox="false" titlebar="false">
    <label name="lblTitle" font-id="fontTitle" visible="true">
      <align>center</align>
      <x>0</x>
      <y>15</y>
      <w>320</w>
      <h>40</h>
      <caption>-Confirm-</caption>
    </label>
    <label name="lblMessage" font-id="fontStandard" visible="true" backcolor="C8C8FA">
      <align>center</align>
      <x>0</x>
      <y>90</y>
      <w>320</w>
      <h>30</h>
      <caption>Cancel checking for this customer?</caption>
    </label>
    <label name="lblCustomerCode" font-id="fontSmall" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>6</x>
      <y>160</y>
      <w>300</w>
      <h>24</h>
      <caption>%ShippingCustomerOutDataSheet.CUSTOMER_CODE%</caption>
    </label>
    <label name="lblCustomerName" font-id="fontSmall" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>6</x>
      <y>184</y>
      <w>280</w>
      <h>48</h>
      <caption>%ShippingCustomerOutDataSheet.CUSTOMER_NAME%</caption>
    </label>
    <button name="Ent" font-id="fontBtn" visible="true" backcolor="FFA500" enabled="true" inputchecked="false">
      <x>0</x>
      <y>275</y>
      <w>160</w>
      <h>45</h>
      <caption>Yes</caption>
      <event>ShpCancel_Ent</event>
    </button>
    <button name="Back" font-id="fontBtn" visible="true" backcolor="FFA500" enabled="true" inputchecked="false">
      <x>160</x>
      <y>275</y>
      <w>160</w>
      <h>45</h>
      <caption>No</caption>
      <event>ShpCancel_Back</event>
    </button>
    <label name="panel" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>0</x>
      <y>70</y>
      <w>324</w>
      <h>168</h>
    </label>
  </form>
</HaiSurf.Page>
