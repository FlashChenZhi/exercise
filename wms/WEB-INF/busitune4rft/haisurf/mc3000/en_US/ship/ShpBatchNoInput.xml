  <font name="fontTitle" fontname="Tahoma" bold="true" italic="false" size="16" bht_size="1" />
  <scan type="focus" />
  <form color="E6E6FA" wave="%CommonDataSheet.BeepWav%" vibration="false" bht-alarm="non" bht-led="non" ce-alarm="non" ce-led="non" controlbox="false" titlebar="false">
