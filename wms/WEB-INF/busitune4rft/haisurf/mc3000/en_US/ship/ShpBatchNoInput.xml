    <button name="List" font-id="fontBtn" visible="true" backcolor="FFA500" enabled="true" inputchecked="false">
      <x>216</x>
      <y>0</y>
      <w>108</w>
      <h>45</h>
      <caption>List</caption>
      <event>BatchI_List</event>
    </button>
    <label name="panel" visible="true" backcolor="C8C8FA">
      <align>left</align>
      <x>0</x>
      <y>110</y>
      <w>324</w>
      <h>200</h>
